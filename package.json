  "scripts": {
    "test": "jest --runInBand --config .jest/config.json",
    "test-coverage": "npm run test -- --coverage --watchAll=false",
    "start": "cross-env NODE_ENV=develop webpack-dev-server --history-api-fallback --watch --port 3000 --mode development --hotOnly",
    "start:localnetwork": "cross-env NODE_ENV=develop webpack-dev-server --host 0.0.0.0 --history-api-fallback --watch --port 3000 --mode development --hotOnly",
    "start:worker": "npm run start -- --service-worker",
    "build": "webpack -p",
    "watch": "webpack -d --watch",
    "storybook": "start-storybook -p 9002",
    "codegen": "apollo codegen:generate --target=typescript gqlTypes",
    "codegen-sdk": "apollo codegen:generate --target=typescript gqlTypes --config src/@sdk/apollo.config.js --globalTypesFile src/@sdk/gqlTypes/globalTypes.ts",
    "codegen-watch": "npm run codegen -- --watch",
    "tslint": "tslint 'src/**/*.ts?(x)'",
    "eslint": "npx eslint  -c .eslintrc.js --ext .ts,.tsx src",
    "eslint-fix": "npx eslint  -c .eslintrc.js --ext .ts,.tsx src --fix",
    "tsc": "npx tsc --noEmit & npx tsc --noEmit -p src/@sdk & npx tsc --noEmit -p src/@next",
    "sitemap": "ts-node ./src/sitemap-builder.ts",
    "test:update:snapshot": "jest --updateSnapshot --config .jest/config.json",
    "postinstall": "husky install",
    "prettier": "prettier --config ./.prettierrc --write './**/*.{js,ts,jsx,tsx}' --loglevel=warn",
    "pretty-quick": "pretty-quick --staged",
    "lhci": "npm run build && npx lhci autorun --config=./lighthouserc.js",
    "serve-dist": "serve dist"
  }
